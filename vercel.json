{
  "version": 2,
  "name": "card-accounting-system",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/archive",
      "dest": "/archive.html"
    },
    {
      "src": "/report",
      "dest": "/report.html"
    }
  ]
}
